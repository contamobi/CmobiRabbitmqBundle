{
    "name": "cmobi/rabbitmq-bundle",
    "description": "Bundle to manager RabbitMQ message broker using phpamqplib",
    "type": "symfony-bundle",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Cmobi\\RabbitmqBundle\\": ""
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.9",
        "symfony/http-kernel": "2.8.*",
        "php-amqplib/php-amqplib": "2.5.*",
        "symfony/console": "^2.8"
    },
    "require-dev": {
        "symfony/framework-bundle": "~2.8",
        "phpunit/phpunit": "5.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
